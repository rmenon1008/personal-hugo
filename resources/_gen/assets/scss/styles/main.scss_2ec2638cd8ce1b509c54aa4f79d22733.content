@font-face{font-family:"Atkinson Hyperlegible";src:url("/fonts/Atkinson/Atkinson-Hyperlegible-Regular-102a.woff2") format("woff2"),url("/fonts/Atkinson/Atkinson-Hyperlegible-Regular-102.woff") format("woff");font-weight:normal}body{margin:0;padding:0;font-family:"Atkinson Hyperlegible"}h1{font-family:"HoudiniTrio","Helvetica Neue",Helvetica,Arial,sans-serif;font-weight:normal !important;text-transform:lowercase;letter-spacing:-0.02em;font-size:3.5rem}.content{padding:0 4rem}header{padding:4rem 0 2rem 0}header .header{padding-bottom:0.5rem;border-bottom:1.5px solid black;display:flex;gap:1rem}header .header .path-title{flex:1}header .header .path-title h1{margin:0;margin-top:1.2rem}header .header .path-title .path{font-family:"HoudiniTrio","Helvetica Neue",Helvetica,Arial,sans-serif;font-weight:normal !important;text-transform:lowercase;letter-spacing:-0.02em;font-size:1.3rem;display:flex;color:#888}header .header .path-title .path a{text-decoration:none;color:#888}header .header .path-title .path a:hover{color:black}header .header .nav-date{text-align:right;position:relative}header .header .nav-date ul{display:flex;flex-direction:row;justify-content:flex-end;font-size:1.3rem;gap:1.5em;padding:0;margin:0}header .header .nav-date ul li{font-family:"HoudiniTrio","Helvetica Neue",Helvetica,Arial,sans-serif;font-weight:normal !important;text-transform:lowercase;letter-spacing:-0.02em;list-style-type:none}header .header .nav-date ul li a{text-decoration:none;color:#888}header .header .nav-date ul li a:hover{color:black}header .header .nav-date time{white-space:nowrap;position:absolute;right:0;bottom:0}
